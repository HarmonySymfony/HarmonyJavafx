<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml" stylesheets="@java.css"
            fx:controller="controllers.AjouterAnalyse"
            prefHeight="400.0" prefWidth="600.0">

    <VBox alignment="CENTER" spacing="10">
        <Label text="Ajouter une Analyse" />
        <TextField fx:id="prixTextField" promptText="Prix" />
        <TextField fx:id="typeTextField" promptText="Type" />
        <Button text="Ajouter" onAction="#ajouterAnalyse" />
    </VBox>

</AnchorPane>
