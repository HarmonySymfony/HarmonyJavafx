<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="560.0" prefWidth="805.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AfficherLaboratoire">
    <TableView fx:id="laboratoireTable" layoutX="132.0" layoutY="96.0" prefHeight="450" prefWidth="521.0" visible="true">

        <columns>
            <TableColumn fx:id="idColumn" prefWidth="75" text="ID">
                <cellValueFactory>
                    <PropertyValueFactory property="id" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="nomColumn" prefWidth="100" text="nom">
                <cellValueFactory>
                    <PropertyValueFactory property="nom" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="emplacementColumn" prefWidth="100" text="emplacement">
                <cellValueFactory>
                    <PropertyValueFactory property="emplacement" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="actionsColumn" prefWidth="245.0" text="actions">
                <cellFactory>
                    <PropertyValueFactory property="DUMMY" />
                </cellFactory>
            </TableColumn>

        </columns>
    </TableView>
   <Button fx:id="ajouterLaboratoire" layoutX="539.0" layoutY="56.0" mnemonicParsing="false" onAction="#addLaboratoire" text="Ajouter laboratoire" />
   <Button layoutX="74.0" layoutY="56.0" mnemonicParsing="false" onAction="#ajouteranalyse" text="ajouter Analyse" />

</AnchorPane>
