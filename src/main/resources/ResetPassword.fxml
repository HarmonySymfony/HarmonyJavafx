<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ResetPasswordDialog">
    <TextField fx:id="emailField" layoutX="50.0" layoutY="25.0" promptText="Email" />
    <PasswordField fx:id="tempPasswordField" layoutX="50.0" layoutY="50.0" promptText="Temporary Password" />
    <PasswordField fx:id="newPasswordField" layoutX="50.0" layoutY="100.0" promptText="New Password" />
    <PasswordField fx:id="confirmPasswordField" layoutX="50.0" layoutY="150.0" promptText="Confirm Password" />
    <Button layoutX="50.0" layoutY="200.0" mnemonicParsing="false" onAction="#submitResetPassword" text="Submit" />
</AnchorPane>
